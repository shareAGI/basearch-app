{
  "manifest_version": 3,
  "name": "Adx24",
  "version": "0.0.0",
  "icons": {},
  "chrome_url_overrides": {
    "newtab": "index.html#/start"
  },
  "content_scripts": [
    {
      "matches": ["http://*/*", "https://*/*"],
      "js": ["{content}"]
    }
  ],
  "background": {
    "service_worker": "{background}"
  },
  "permissions": ["tabs", "bookmarks"]
}
